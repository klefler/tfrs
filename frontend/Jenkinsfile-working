stage('FT on Dev') {

    
    podTemplate(label: "develop-frontend-bddstack-${env.BUILD_NUMBER}", name: "develop-frontend-bddstack-${env.BUILD_NUMBER}", serviceAccount: 'jenkins', cloud: 'openshift',
        containers: [
            containerTemplate(
                name: 'jnlp',
                image: '172.50.0.2:5000/mem-tfrs-tools/bddstack-tfrs:latest',
                resourceRequestCpu: '500m',
                resourceLimitCpu: '1000m',
                resourceRequestMemory: '2Gi',
                resourceLimitMemory: '4Gi',
                workingDir: '/home/jenkins',
                command: '',
                args: '${computer.jnlpmac} ${computer.name}'
            )
        ]
    ) {
    node("develop-frontend-bddstack-${env.BUILD_NUMBER}") {
 withCredentials([
    usernamePassword(credentialsId: 'SUPPLIER_ONE', usernameVariable: 'supplier_one_username', passwordVariable: 'supplier_one_password'),
    usernamePassword(credentialsId: 'SUPPLIER_TWO', usernameVariable: 'supplier_two_username', passwordVariable: 'supplier_two_password')
]){
            sh '''
                set +x
                echo "supplier_one_password=$supplier_one_password" 
                echo "supplier_two_password=$supplier_two_password" 
               '''
}

echo "supplier_one_password11=$supplier_one_password" 

} //end of node

    } //end of podTemplate 
} //end of stage
